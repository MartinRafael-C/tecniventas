{
  "expo": {
    "name": "tecniventas-app",
    "slug": "tecniventas-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tecniventas.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tecniventas.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "tecniventas"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-native": "^0.72.8",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "expo": "^54.0.25",
    "expo-linear-gradient": "~15.0.7",
    "expo-router": "~6.0.15",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "react-native-web": "^0.21.0",
    "zod": "^4.1.13"
  }
}
